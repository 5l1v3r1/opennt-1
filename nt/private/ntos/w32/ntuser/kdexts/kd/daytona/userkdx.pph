void
EnterWowCritSect(
    PTHREADINFO pti,
    PWOWPROCESSINFO pwpi
    )
{
   if (!++pwpi->CSLockCount) {
       pwpi->CSOwningThread = pti;
       return;
       }

   



do { if (VRipOutput(0, (0x10000000) | 0x000c0000, "..\\..\\..\\kernel\\userk.h", 4005, "MultipleWowTasks running simultaneously %x %x\n", pwpi->CSOwningThread, pwpi->CSLockCount)) { DbgBreakPoint(); } } while (0);

   return;
}

_inline
void
ExitWowCritSect(
    PTHREADINFO pti,
    PWOWPROCESSINFO pwpi
    )
{
   if (pti == pwpi->CSOwningThread) {
       pwpi->CSOwningThread = ((void *)0);
       pwpi->CSLockCount--;
       }

   return;
}



















